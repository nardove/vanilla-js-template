		"url": "https://github.com/nardove/vanilla-js-template/issues"
	},
	"homepage": "https://github.com/nardove/vanilla-js-template#readme",
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/plugin-transform-runtime": "^7.11.5",
		"@babel/runtime-corejs2": "^7.11.2",
		"sass": "^1.26.10"
	}
